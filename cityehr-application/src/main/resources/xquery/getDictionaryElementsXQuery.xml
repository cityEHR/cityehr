<query xml:space="preserve">
        xquery version "1.0"; 
        declare namespace iso-13606="http://www.iso.org/iso-13606"; 
        declare namespace cda="urn:hl7-org:v3";
        
        for $element in //iso-13606:element<elementClause/>
        return
        $element
</query>

